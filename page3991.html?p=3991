<script>
// Disable Elementor add-ons and remove script libraries
function disableElementorAddons() {
    // Get all Elementor widgets
    var elementorWidgets = document.querySelectorAll('.elementor-widget');

    // Loop through each widget
    elementorWidgets.forEach(function(widget) {
        // Check if the widget has any add-on classes
        var addonClasses = widget.className.match(/elementor-addon-[a-zA-Z0-9_-]+/g);

        // If add-on classes are found, remove them
        if (addonClasses) {
            addonClasses.forEach(function(addonClass) {
                widget.classList.remove(addonClass);
            });
        }
    });

    console.log('All Elementor add-ons disabled.');

    // Remove script libraries
    var scriptTags = document.querySelectorAll('script[src]');
    scriptTags.forEach(function(scriptTag) {
        // Check if the script is not being used at the moment
        if (!scriptTag.parentNode.classList.contains('elementor-edit-area')) {
            scriptTag.remove();
            console.log('Script library removed:', scriptTag.src);
        }
    });
}

// Call the function to disable Elementor add-ons and remove script libraries when the page loads
document.addEventListener('DOMContentLoaded', function() {
    disableElementorAddons();
});

</script>

<script>
    function disableImgDragging() {
	var images = document.getElementsByTagName("img");
	for(var i = 0 ; i < images.length ; i++) {
		images[i].classList.add('no-drag');
		images[i].setAttribute('no-drag', 'on');
		images[i].setAttribute('draggable', 'false');
		images[i].addEventListener('dragstart', function( event ) {
			event.preventDefault();
		}, false);	
	}
}
disableImgDragging();
</script>

<script>
    function disableHorizontalScroll() {
  var isMobile = /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent);

  if (isMobile) {
    document.body.style.touchAction = 'pan-y';
  }
}

disableHorizontalScroll();
</script>


