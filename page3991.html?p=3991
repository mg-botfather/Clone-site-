			<script>
  $(document).ready(function(){
    $('body').addClass('body-loaded');
    $('a[href]:not([href^="#"]):not([href^="tel"])').click(function(event){
      if (!event.ctrlKey && !event.shiftKey && !event.metaKey && !$(this).attr('target')) {
        event.preventDefault();
        var url = $(this).attr('href');
        $('body').addClass('body-loader');
        gsap.to('body::before, body::after', { height: '100%', duration: 0.8, onComplete: function() {
          window.location.href = url;
        }});
      }
    });
  });
</script>





