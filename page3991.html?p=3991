    document.addEventListener("DOMContentLoaded", function() {
        var checkInterval = setInterval(function() {
            var utmInput = document.getElementById("form-field-utm_dict");
            if (utmInput) {
                var value = utmInput.value.trim();
                try {
                    console.log(value);
                    var utmData = JSON.parse(value);
                    if (Object.keys(utmData).length > 0) {
                        document.cookie = "utm_dict=" + encodeURIComponent(value) + "; path=/; max-age=" + (7 * 24 * 60 * 60);
                        localStorage.setItem("utm_dict", value);
                        clearInterval(checkInterval); // stop the loop
                    }
                } catch (e) {
                    // Invalid JSON, ignore and keep checking
                }
            }
        }, 1000);
    });
